[ req ]
default_bits       = 2048
default_md         = sha256
default_keyfile    = server.key
distinguished_name = req_distinguished_name
req_extensions     = req_ext
prompt             = no

[ req_distinguished_name ]
C  = TW
ST = Taiwan
L  = Taipei
O  = Company
OU = Department
CN = 127.0.0.1

[ req_ext ]
subjectAltName = @alt_names

[ v3_ext ]
subjectAltName = @alt_names

[ alt_names ]
IP.1 = 127.0.0.1
